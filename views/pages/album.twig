{{ include('partials/header.twig') }}

<div id="album">
    <h2>{{ album.name }}</h2>
    <p>{{ album.year }}</p>
    
    <div class="boxAlbum">
        <img src="../assets/img/{{ album.picture }}" alt="" width="300">

        {% for song in album.songs %}
        <p>{{song}}</p>
        {% endfor %}
    </div>
</div>

{{ include('partials/footer.twig') }}